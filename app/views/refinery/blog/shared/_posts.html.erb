<% if @posts.many? %>
  <h2><%= t('.other') %></h2>
  <ul id="blog_posts">
    <% @posts.each do |blog_post| %>
      <li class='clearfix'>
        <%= link_to blog_post.title, refinery.blog_post_path(blog_post) %> - <%= blog_post.created_at.strftime("%m/%d/%Y") %>
      </li>
    <% end %>
  </ul>
<% end %>