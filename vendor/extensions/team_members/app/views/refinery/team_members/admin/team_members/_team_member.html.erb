<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(team_member) -%>">
  <span class='title'>
    <%= team_member.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_team_members_admin_team_member_path(team_member),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.team_members_admin_team_member_path(team_member),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => team_member.name),
        :method => :delete %>
  </span>
</li>
